{% extends 'layout.html.twig' %}

{% block content %}
    <h1>Teams</h1>
    <ul>
        {% for team in teams %}
            <li><a href="/team/{{ team.id }}">{{ team.id }}. {{ team.name }}</a></li>
        {% else %}
            <li>Nothing to display</li>
        {% endfor %}
    </ul>
{% endblock %}
